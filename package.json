{
  "name": "csharp-dependency-graph",
  "displayName": "C# Dependency Graph",
  "description": "Generates a graph of your project dependencies",
  "publisher": "danilocolombi",
  "repository": {
    "type": "git",
    "url": "https://github.com/danilocolombi/vscode-csharp-dependency-graph"
  },
  "license": "MIT",
  "icon": "images/logo.png",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csharp-dependency-graph.generate",
        "title": "Generate Dependency Graph",
        "arguments": {
          "name": "filePath",
          "description": "The file path to save the graph."
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "^20.16.10",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  }
}
